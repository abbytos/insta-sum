{
    "env": {
      "browser": true,
      "es2021": true,
      "webextensions": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "parser": "@babel/eslint-parser"
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended"
    ],
    "plugins": [
      "vue",
      "import"
    ],
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "warn"
      // Add more rules as needed
    }
  }
  